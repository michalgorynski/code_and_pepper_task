{
  "compilerOptions": {
    "types": ["cypress", "@testing-library/cypress", "node"],
    "isolatedModules": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es2015",
    "downlevelIteration": true
  },
  "include": ["./**/*.ts", "./**/*.js"],
  "exclude": ["../node_modules"]
}
